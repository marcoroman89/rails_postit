<%= render 'shared_partials/errors', object: @post %>

<%= form_for @post do |f| %>
  <%= f.label :title %><br/>
  <%= f.text_field :title %><br/>
  <br/>
  <%= f.label :url %><br/>
  <%= f.text_field :url %><br/>
  <br/>
  <%= f.submit @post.new_record? ? "Create Post" : "Update Post", class: 'btn btn-primary' %>
<% end %>
