<h2>Register</h2>
<%= render 'shared_partials/errors', errors_object: @user %>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %><br/>

  <%= f.submit "Register", class: "btn btn-primary" %>
 <% end %>
